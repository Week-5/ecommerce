<section>
  {{#each data.items}}
      <div class="grid-container">
      <a href="/items/{{this.id}}/">
        <img class="allItemsImage" src="{{this.image}}" title="Click Here To View Options"/>
      </a>
      <div style="color: #757575; font-family: Arial, Helvetica, sans-serif; font-style: italic; margin: auto, padding: 0;">
        {{this.title}}
      </div>
            </div>

  {{/each}}
</section>